                         where ACCOUNT_ID IS NOT NULL 
                              AND DS=(select MAX(DS) from "IMMUTA_DSS_PROD"."DISNEY_PLUS"."DIM_DISNEY_SUBSCRIPTIONS") 
                              and (IS_PRIMARY = 1)
                              and (	SUBSCRIPTION_STATE = 'paid')
                              
                              and ACCOUNT_HOME_COUNTRY in ('CW','BQ','CO','AI','PE','VC','AR','AG','BS','SR','DM','CR','PY',
